{
  "name": "kit",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:base": "tsc && vite build",
    "build": "cross-env NODE_ENV=production npm run build:base",
    "build:analyze": "cross-env ANALYZE=1 npm run build && node scripts/build-optimizer.mjs",
    "analyze": "cross-env ANALYZE=1 npm run build",
    "build:production": "npm run build && npm run optimize",
    "preview": "vite preview",
    "deps:apply": "node scripts/deps-replacer.mjs apply --plan ./deps-plan.json",
    "deps:pr": "node scripts/deps-replacer.mjs pr --plan ./deps-plan.json",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:clean": "cargo clean -p kit_lib --manifest-path src-tauri/Cargo.toml || true && cargo clean -p kit --manifest-path src-tauri/Cargo.toml || true",
    "tauri:build:ci": "TAURI_BUILD_TARGET_DIR=${TAURI_BUILD_TARGET_DIR:-src-tauri/target} tauri build",
    "optimize": "node scripts/build-optimizer.mjs",
    "publish": "node scripts/updater.mjs",
    "changelog": "node scripts/changelog-generator.mjs",
    "changelog:generate": "node scripts/changelog-generator.mjs generate",
    "changelog:release": "node scripts/changelog-generator.mjs release-notes",
    "changelog:test": "node scripts/test-changelog.mjs",
    "release:patch": "npm version patch && npm run changelog:generate && git add CHANGELOG.md && git commit -m 'docs: update changelog' && git push --tags",
    "release:minor": "npm version minor && npm run changelog:generate && git add CHANGELOG.md && git commit -m 'docs: update changelog' && git push --tags",
    "release:major": "npm version major && npm run changelog:generate && git add CHANGELOG.md && git commit -m 'docs: update changelog' && git push --tags",
    "clean": "rm -rf dist src-tauri/target node_modules/.vite",
    "ci:build:analyze": "cross-env ANALYZE=1 npm run build && node scripts/build-optimizer.mjs",
    "clean:all": "npm run clean && rm -rf node_modules",
    "ci:check-tools": "node scripts/check-tools.mjs"
  },
  "dependencies": {
    "@actions/github": "^6.0.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.6",
    "@tanstack/react-virtual": "^3.0.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fflate": "^0.8.2",
    "gifuct-js": "^2.1.2",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsbarcode": "^3.12.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.544.0",
    "mermaid": "^11.12.0",
    "motion": "^12.23.21",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5",
    "web-vitals": "^4.2.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.132.6",
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.5.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "cross-env": "^7.0.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.44.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.7"
  }
}
